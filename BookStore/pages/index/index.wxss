.index-swiper{
  height: 10%;
}
.index-swiper swiper{   
  width: 750rpx;
  height: 280rpx;   
}
.index-swiper swiper image{   
  width: 100%;
  height: 100%;
}
.index-cate{
  display: flex;
}
.index-cate navigator{
  flex:1;
  padding: 20rpx;
}
.index-cate navigator image{
  width: 100%;
}
.index-floor{

}
.floor-group{

}
.floor-title{
 
}
.floor-title image{
  width: 100%;
}
.floor-list{
  /* 清除浮动 */
  overflow: hidden;
}
.floor-list navigator{
  float: left;
  width: 33.33%;
  padding:5rpx;
}
/* 后四个的高度    后几个就（-n-几） */
.floor-list navigator:nth-last-child(-n+4){
  height: 27.72711207vw;
  border-left: 1rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3) {
  border-bottom: 1rpx solid #fff;
}

.floor-list navigator image{
  width: 100%;
  height: 100%;
}








